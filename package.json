{
  "name": "simple-protractor-jasmine-framework",
  "version": "1.0.0",
  "description": "A simple test framework with reporting and multiple environment support",
  "main": "index.js",
  "scripts": {
    "wd:update": "./node_modules/.bin/webdriver-manager update",
    "wd:start": "./node_modules/.bin/webdriver-manager start",
    "specs:dev": "cross-env NODE_ENV=dev ./node_modules/.bin/protractor conf/conf.js",
    "specs:test": "cross-env NODE_ENV=test ./node_modules/.bin/protractor conf/conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TechsterPank/simple-protractor-jasmine-framework"
  },
  "keywords": [],
  "author": "Pankaj",
  "license": "ISC",
  "dependencies": {
    "protractor": "^5.4.2",
    "protractor-beautiful-reporter": "^1.3.6"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
